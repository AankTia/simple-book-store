<p style="color: green"><%= notice %></p>

<p>
<%= button_to("Log Out", destroy_user_session_path, method: :delete ) %>
</p>

<h1>Users</h1>

<%= link_to "New user", new_user_path %>

<div id="users">
  <table>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Action</th>
    </tr>
    <% @users.each do |user| %>
      <%= render user %>
    <% end %>
  </table>
  
  <%= paginate @users %>
</div>
